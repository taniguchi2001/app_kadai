<html>
<h1>Books</h1>

<head></head>
<body>
  <p id="notice"></p>
  <table>
    <thead>
      <tr>
        <th>Title</th>
        <th>Body</th>
        <th colspan="3"></th>
      </tr>
    <thead>
      <tbody></tbody>
  </table>
  <h2>New Book</h2>
  <h1>新規投稿</h1>
<!-- form_with部分 -->
  <%= form_with model: @book, url:'/books', local:true do |f| %>
  <h4>Title</h4>
  <%= f.text_field :title %>
  <h4>Body</h4>
  <%= f.text_area :body %>
  <%= f.submit 'New Book' %>
  <% end %>
  </body>
  </html>